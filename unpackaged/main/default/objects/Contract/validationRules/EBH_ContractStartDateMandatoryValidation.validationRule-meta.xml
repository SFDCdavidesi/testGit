<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EBH_ContractStartDateMandatoryValidation</fullName>
    <active>false</active>
    <description>US-0024, US-0007080</description>
    <errorConditionFormula>AND(
ISPICKVAL(Status, &#39;Executed&#39;),
ISBLANK(StartDate)) &amp;&amp; NOT( $Setup.ActiveValidationRules__c.All_Validation_Rules_Deactivated__c )</errorConditionFormula>
    <errorDisplayField>StartDate</errorDisplayField>
    <errorMessage>Please provide the Contract Start Date when the contract status is changed to &quot;Executed&quot;.</errorMessage>
</ValidationRule>
