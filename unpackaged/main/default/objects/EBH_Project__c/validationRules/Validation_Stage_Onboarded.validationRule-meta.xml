<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_Stage_Onboarded</fullName>
    <active>true</active>
    <description>US-0007728 - [AU] MP Restrict Project Stage Updates</description>
    <errorConditionFormula>AND(
RecordType.DeveloperName=&#39;ManagedPayment&#39;,
$Profile.Name != &#39;System Administrator&#39;,
AND($Profile.Name!=&#39;System Administrator&#39;,$User.Id!=&#39;0056A000000yx6iQAA&#39;),
RegistrationCountry__c=&#39;Australia&#39;,
ISPICKVAL(PRIORVALUE(EBH_Stage__c), &#39;Onboarded&#39;),
NOT($Setup.ActiveValidationRules__c.All_Validation_Rules_Deactivated__c))</errorConditionFormula>
    <errorMessage>Please note once a stage has reached &quot;Onboarded&quot; changes can only be made by Admin</errorMessage>
</ValidationRule>
