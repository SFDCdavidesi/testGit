<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>V2_MultiSKUPopulated</fullName>
    <active>true</active>
    <description>US-0008843 - [NA] Migrate NA Deals Validation Rules to Hive</description>
    <errorConditionFormula>AND(
OR(
NOT(ISBLANK(TEXT(SellersDealPriceMSKULower__c))),
NOT(ISBLANK(TEXT(SellersDealPriceMSKUUpper__c))),
NOT(ISBLANK(TEXT(SellersOfferPriceMSKULower__c))),
NOT(ISBLANK(TEXT(SellersOfferPriceMSKUUpper__c)))
),
OR(
ISBLANK(TEXT(SellersDealPriceMSKULower__c)),
ISBLANK(TEXT(SellersDealPriceMSKUUpper__c)), ISBLANK(TEXT(SellersOfferPriceMSKULower__c)),
ISBLANK(TEXT(SellersOfferPriceMSKUUpper__c))
),
RecordType.DeveloperName =&#39;Deal_V2&#39;,
NOT($Setup.byPass__c.ByPass_Validation__c) 
)</errorConditionFormula>
    <errorMessage>All Seller and Deal Price Multi SKU information must be populated</errorMessage>
</ValidationRule>
