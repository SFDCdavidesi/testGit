<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>V2_Quantity_per_Purchaser_Greater_than_0</fullName>
    <active>true</active>
    <description>US-0008843 - [NA] Migrate NA Deals Validation Rules to Hive</description>
    <errorConditionFormula>AND(
NOT($Setup.byPass__c.ByPass_Validation__c),
RecordType.DeveloperName =&#39;Deal_V2&#39;,
AND(NOT(ISBLANK(EBH_MaximumPurchases__c)),
OR(EBH_MaximumPurchases__c = 0,
EBH_MaximumPurchases__c &gt; EBH_Quantity__c
)))</errorConditionFormula>
    <errorMessage>Maximum Purchases should be greater than 0 and less than the total Quantity of the Deal</errorMessage>
</ValidationRule>
