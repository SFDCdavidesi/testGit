<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>V2_Seller_Approver_must_be_populated</fullName>
    <active>true</active>
    <description>US-0008843 - [NA] Migrate NA Deals Validation Rules to Hive</description>
    <errorConditionFormula>AND(
Backdated_Deal__c = false,
ISBLANK(Seller_Approver_1__c),
NOT($Setup.byPass__c.ByPass_Validation__c),
RecordType.DeveloperName =&#39;Deal_V2&#39;
)</errorConditionFormula>
    <errorDisplayField>Seller_Approver_1__c</errorDisplayField>
    <errorMessage>At least 1 Seller Approver must be populated</errorMessage>
</ValidationRule>
