<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Discount_p__c</fullName>
    <description>US-0007055 AU and EU] API Call to auto populate &quot;Current seller price&quot; in Deal</description>
    <externalId>false</externalId>
    <formula>( Current_Seller_Price__c  -  EBH_DealPrice__c  ) / Current_Seller_Price__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This is the % discount from Current eBay seller Price to the Deal Price</inlineHelpText>
    <label>Discount %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
