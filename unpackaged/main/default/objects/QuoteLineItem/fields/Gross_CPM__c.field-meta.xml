<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Gross_CPM__c</fullName>
    <description>US-0007824 - EU Advertising CPM Calculator</description>
    <externalId>false</externalId>
    <formula>IF(
ISPICKVAL(Billing_Category__c, &quot;CPM&quot;),
PricebookEntry.UnitPrice * 1000,
PricebookEntry.UnitPrice)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Gross CPM</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
