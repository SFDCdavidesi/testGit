        <triggerType>onCreateOnly</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Seller_Declined_Reason</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Update_Coupon_Seller_Stage</name>
