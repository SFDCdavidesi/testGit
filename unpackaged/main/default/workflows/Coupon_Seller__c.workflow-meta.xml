    <rules>
        <fullName>AU Contract Breach</fullName>
        <actions>
            <name>AU_Contract_Breach</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>US-0018788: [AU] Contract Breach Email Notification,
US-0010176 - MC: Coupon email notifications to external users</description>
        <formula>AND(ISPICKVAL( Coupon_Seller_Stage__c , &quot;Seller Declined&quot;) , ISPICKVAL( Seller_Declined_Reasons__c , &quot;Contract Breach&quot;) , ISPICKVAL( Coupon__r.Contract_Language__c , &quot;AU&quot;), !MC_Whitelisted__c)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Approved Coupon sellers to UK coupon seller owner</fullName>
        <actions>
            <name>UK_Coupon_Approval_Email</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Coupon_Seller__c.Coupon_Seller_Stage__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <criteriaItems>
            <field>Coupon_Seller__c.Contract_Language__c</field>
            <operation>equals</operation>
            <value>UK</value>
        </criteriaItems>
        <criteriaItems>
            <field>Coupon_Seller__c.MC_Whitelisted__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>US-0007173: UK Coupon Approval Email - to Coupon Seller Owner [NOTE: THIS IS NOT STANDARD SF APPROVAL FLOW],
US-0010176 - MC: Coupon email notifications to external users</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Clear Declined Reason When sending Contract</fullName>
        <actions>
            <name>Update_Breach_Reason</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_SELLER_DECLINED_REASON</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>US-0007208 Coupon Seller - Clear Field &quot;Declined Reason&quot; when sending contract</description>
        <formula>AND(ISCHANGED( Coupon_Seller_Stage__c ) , NOT(ISPICKVAL(Coupon_Seller_Stage__c, &#39;Seller Declined&#39;)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Cofund Date Assignment</fullName>
        <actions>
            <name>AssignCofundEndDateFromCoupon</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>AssignCofundStartDateFromCoupon</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>[EPH-6086] have coupon details in Hive  (Depreciated. moved to Flow)</description>
        <formula>OR( NOT(ISNULL(Coupon__r.Coupon_End_Date__c)), NOT(ISNULL(Coupon__r.Coupon_Start_Date__c))  )</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Contract Void</fullName>
        <active>false</active>
        <description>US-0006482 Coupon - Contract Voiding for DE, UK, FR, IT &amp; ES</description>
        <formula>AND( OR(ISPICKVAL( Coupon__r.Contract_Language__c , &quot;DE&quot;),ISPICKVAL( Coupon__r.Contract_Language__c , &quot;UK&quot;),ISPICKVAL( Coupon__r.Contract_Language__c , &quot;FR&quot;),ISPICKVAL( Coupon__r.Contract_Language__c , &quot;IT&quot;),ISPICKVAL( Coupon__r.Contract_Language__c , &quot;ES&quot;)) , OR( Coupon__r.RecordType.DeveloperName = &quot;Category_Based&quot;, Coupon__r.RecordType.DeveloperName = &quot;Item_Based&quot;),  ISBLANK(Contract_Accept_Date__c ) )</formula>
        <triggerType>onCreateOnly</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Seller_Declined_Reason</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Update_Coupon_Seller_Stage</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Coupon_Seller__c.Coupon_Contract_Due_Date__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
