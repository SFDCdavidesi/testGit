    <rules>
        <fullName>Brand Lift Study Submitted</fullName>
        <actions>
            <name>Brand_Lift_Study_Submitted</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Advertising_Request_Portal__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Brand Lift Study</value>
        </criteriaItems>
        <criteriaItems>
            <field>Advertising_Request_Portal__c.Status__c</field>
            <operation>equals</operation>
            <value>Submitted for Approval</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CIA Request Alert</fullName>
        <actions>
            <name>CIA_Request</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(  $Setup.byPass__c.byPass_WFRule__c = FALSE, 
RecordType.Name = &#39;CSI Requests&#39;)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>CSI Request Approved</fullName>
        <actions>
            <name>CSI_Request_Approved</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(  $Setup.byPass__c.byPass_WFRule__c = FALSE,  RecordType.Name = &#39;CSI Requests&#39;,  ISPICKVAL( Status__c, &#39;Complete&#39; )  )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CSI Survey to Offshore</fullName>
        <actions>
            <name>CSI_Survey_to_Offshore</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(  $Setup.byPass__c.byPass_WFRule__c = FALSE,  RecordType.Name = &#39;CSI Requests&#39;, ISPICKVAL( Country__c , &#39;US&#39;), ISPICKVAL( Status__c , &#39;Data Sent back to CSI&#39;)   )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
